{
  "name": "hexopress",
  "version": "1.2.0",
  "private": true,
  "description": "A client software for Hexo blog.",
  "license": "MIT",
  "author": {
    "name": "Charles",
    "email": "charlestang@foxmail.com"
  },
  "main": ".vite/build/main.js",
  "scripts": {
    "check-all": "concurrently -c \"bgBlue.bold,bgMagenta.bold,bgCyan.bold,bgGreen.bold\" npm:vue-check npm:vitest-check npm:node-check npm:conf-check",
    "conf-check": "tsc --noEmit -p tsconfig/tsconfig.tools.json",
    "dev": "electron-forge start",
    "format": "prettier --write src/ main/ types/ *.ts *.json *.md",
    "lint": "eslint . --fix",
    "make": "electron-forge make",
    "node-check": "tsc --noEmit -p tsconfig/tsconfig.node.json",
    "package": "electron-forge package",
    "publish": "electron-forge publish",
    "start": "electron-forge start",
    "test": "vitest",
    "vitest-check": "vue-tsc --noEmit -p tsconfig/tsconfig.vitest.json",
    "vue-check": "vue-tsc --noEmit -p tsconfig/tsconfig.app.json"
  },
  "dependencies": {
    "@fastify/static": "^8.0.1",
    "@replit/codemirror-vim": "^6.2.1",
    "electron-squirrel-startup": "^1.0.1",
    "element-plus": "^2.8.5",
    "fastify": "^5.0.0",
    "hexo": "^7.3.0",
    "hexo-front-matter": "^4.2.1",
    "hexo-fs": "^4.1.3",
    "hexo-util": "^3.3.0",
    "md-editor-v3": "^4.21.0",
    "path-browserify": "^1.0.1",
    "pinia": "^2.2.4",
    "vue": "^3.5.12",
    "vue-i18n": "^10.0.4",
    "vue-router": "^4.4.5",
    "vue3-calendar-heatmap": "^2.0.5"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.5.0",
    "@electron-forge/maker-deb": "^7.5.0",
    "@electron-forge/maker-rpm": "^7.5.0",
    "@electron-forge/maker-squirrel": "^7.5.0",
    "@electron-forge/maker-zip": "^7.5.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.5.0",
    "@electron-forge/plugin-fuses": "^7.5.0",
    "@electron-forge/plugin-vite": "^7.5.0",
    "@electron/fuses": "^1.8.0",
    "@intlify/unplugin-vue-i18n": "^5.2.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/hexo-util": "^0.6.8",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.7.5",
    "@types/path-browserify": "^1.0.3",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/eslint-plugin": "1.1.7",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.1.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "concurrently": "^9.0.1",
    "electron": "^33.0.0",
    "eslint": "^9.12.0",
    "eslint-plugin-vue": "^9.29.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.9",
    "vitest": "^2.1.3",
    "vue-tsc": "^2.1.6",
    "web-storage-cache": "^1.1.1"
  },
  "productName": "HexoPress"
}
