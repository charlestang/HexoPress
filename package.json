{
  "name": "hexopress",
  "main": "main.mjs",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "start": "electron .",
    "electron:dev": "wait-on tcp:5173 && electron .",
    "electron:run": "NODE_ENV=dev concurrently -k \"npm run dev\" \"npm run electron:dev\"",
    "forge:start": "electron-forge start",
    "forge:package": "electron-forge package",
    "forge:make": "electron-forge make",
    "forge:publish": "electron-forge publish"
  },
  "dependencies": {
    "@fastify/static": "^7.0.0",
    "@replit/codemirror-vim": "^6.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "element-plus": "^2.5.4",
    "fastify": "^4.26.0",
    "hexo": "^7.1.1",
    "hexo-front-matter": "^4.2.1",
    "hexo-fs": "^4.1.1",
    "hexo-util": "^3.1.0",
    "md-editor-v3": "^4.8.1",
    "path-browserify": "^1.0.1",
    "pinia": "^2.1.7",
    "vue": "^3.4.15",
    "vue-i18n": "^9.6.2",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "@rushstack/eslint-patch": "^1.5.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/express": "^4.17.21",
    "@types/hexo-util": "^0.6.8",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^20.8.8",
    "@types/path-browserify": "^1.0.2",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.5.1",
    "concurrently": "^8.2.1",
    "electron": "^28.0.0",
    "eslint": "^8.52.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "typescript": "^5.3.2",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.10",
    "vitest": "^1.0.1",
    "vue-tsc": "^1.8.22",
    "wait-on": "^7.1.0",
    "web-storage-cache": "^1.1.1"
  }
}
