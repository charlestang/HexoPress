{
  "name": "hexopress",
  "version": "1.2.1",
  "private": true,
  "description": "A client software for Hexo blog.",
  "license": "MIT",
  "author": {
    "name": "Charles",
    "email": "charlestang@foxmail.com"
  },
  "main": ".vite/build/main.js",
  "scripts": {
    "check-all": "concurrently -c \"bgBlue.bold,bgMagenta.bold,bgCyan.bold,bgGreen.bold\" npm:vue-check npm:vitest-check npm:node-check npm:conf-check",
    "conf-check": "tsc --noEmit -p tsconfig/tsconfig.tools.json",
    "dev": "electron-forge start",
    "format": "prettier --write src/ main/ types/ *.ts *.json *.md",
    "lint": "eslint . --fix",
    "make": "electron-forge make",
    "node-check": "tsc --noEmit -p tsconfig/tsconfig.node.json",
    "package": "electron-forge package",
    "publish": "electron-forge publish",
    "start": "electron-forge start",
    "test": "vitest",
    "vitest-check": "vue-tsc --noEmit -p tsconfig/tsconfig.vitest.json",
    "vue-check": "vue-tsc --noEmit -p tsconfig/tsconfig.app.json"
  },
  "dependencies": {
    "@fastify/static": "^8.1.1",
    "@replit/codemirror-vim": "^6.2.1",
    "electron-squirrel-startup": "^1.0.1",
    "element-plus": "^2.9.6",
    "fastify": "^5.2.1",
    "hexo": "^7.3.0",
    "hexo-front-matter": "^4.2.1",
    "hexo-fs": "^5.0.0",
    "hexo-util": "^3.3.0",
    "md-editor-v3": "^5.3.2",
    "path-browserify": "^1.0.1",
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.2",
    "vue-router": "^4.5.0",
    "vue3-calendar-heatmap": "^2.0.5"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.7.0",
    "@electron-forge/maker-deb": "^7.7.0",
    "@electron-forge/maker-rpm": "^7.7.0",
    "@electron-forge/maker-squirrel": "^7.7.0",
    "@electron-forge/maker-zip": "^7.7.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.7.0",
    "@electron-forge/plugin-fuses": "^7.7.0",
    "@electron-forge/plugin-vite": "^7.7.0",
    "@electron/fuses": "^1.8.0",
    "@intlify/unplugin-vue-i18n": "^6.0.3",
    "@tsconfig/node20": "^20.1.4",
    "@types/hexo-util": "^0.6.8",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.10",
    "@types/path-browserify": "^1.0.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/eslint-plugin": "1.1.37",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.1.2",
    "electron": "^35.0.1",
    "eslint": "^9.21.0",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "unplugin-auto-import": "^19.1.1",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.2.1",
    "vitest": "^3.0.8",
    "vue-tsc": "^2.2.8",
    "web-storage-cache": "^1.1.1"
  },
  "productName": "HexoPress"
}
