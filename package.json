{
  "name": "hexopress",
  "version": "1.3.0",
  "private": true,
  "description": "A client software for Hexo blog.",
  "license": "MIT",
  "author": {
    "name": "Charles",
    "email": "charlestang@foxmail.com"
  },
  "main": ".vite/build/main.js",
  "scripts": {
    "check-all": "concurrently -c \"bgBlue.bold,bgMagenta.bold,bgCyan.bold,bgGreen.bold\" npm:vue-check npm:vitest-check npm:node-check npm:conf-check",
    "conf-check": "tsc --noEmit -p tsconfig/tsconfig.tools.json",
    "dev": "electron-forge start",
    "format": "prettier --write src/ main/ types/ *.ts *.json *.md",
    "lint": "eslint . --fix",
    "make": "electron-forge make",
    "node-check": "tsc --noEmit -p tsconfig/tsconfig.node.json",
    "package": "electron-forge package",
    "publish": "electron-forge publish",
    "start": "electron-forge start",
    "test": "vitest",
    "vitest-check": "vue-tsc --noEmit -p tsconfig/tsconfig.vitest.json",
    "vue-check": "vue-tsc --noEmit -p tsconfig/tsconfig.app.json"
  },
  "dependencies": {
    "@fastify/static": "^9.0.0",
    "@replit/codemirror-vim": "^6.3.0",
    "date-fns": "^4.1.0",
    "electron-squirrel-startup": "^1.0.1",
    "element-plus": "^2.12.0",
    "fastify": "^5.4.0",
    "hexo": "^8.0.0",
    "hexo-front-matter": "^5.0.0",
    "hexo-fs": "^5.0.0",
    "hexo-util": "^4.0.0",
    "md-editor-v3": "^6.0.1",
    "path-browserify": "^1.0.1",
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue-i18n": "^11.1.12",
    "vue-router": "^5.0.2",
    "vue3-calendar-heatmap": "^2.0.5"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-vite": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@intlify/unplugin-vue-i18n": "^11.0.1",
    "@tsconfig/node20": "^20.1.6",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^25.0.1",
    "@types/path-browserify": "^1.0.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.2.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "concurrently": "^9.1.2",
    "electron": "^40.2.1",
    "eslint": "^9.28.0",
    "eslint-plugin-vue": "^10.2.0",
    "jsdom": "^28.0.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "unocss": "^66.2.0",
    "unplugin-auto-import": "^21.0.0",
    "unplugin-vue-components": "^31.0.0",
    "vite": "^7.1.10",
    "vitest": "^4.0.10",
    "vue-tsc": "^3.1.4",
    "web-storage-cache": "^1.1.1"
  },
  "productName": "HexoPress"
}
